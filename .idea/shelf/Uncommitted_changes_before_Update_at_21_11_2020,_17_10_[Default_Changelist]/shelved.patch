+import * as ActionTypes from "../_actions/bots_actions";
+
+export const My_bots_reducer = (state = {
+    bots: []
+}, action) => {
+    switch (action.type) {
+        case ActionTypes.ADD_BOT:
+            return {...state, bots: action.payload};
+        default:
+            return state;
+    }
